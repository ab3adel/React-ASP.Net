{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends":["plugin:prettier/recommended"],
    "rules":{
        "prettier/prettier":[
            "error",
            {
                "endOfLine":"auto"
            }
        ]
    }
}